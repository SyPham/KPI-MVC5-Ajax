@model IEnumerable<KPI.Model.EF.SubNotification>

@{
    ViewBag.Title = "List Notification Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>List Notification Detail</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KPIName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.URL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserID)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KPIName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            <a href=" @Html.DisplayFor(modelItem => item.URL)"><b>** PLEASE CLICK HERE TO CHART PERIOD **</b></a>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserID)
        </td>
    </tr>
}

</table>
